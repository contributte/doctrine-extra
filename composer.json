{
  "name": "nettrine/extra",
  "description": "Extra functions to Doctrine ecosystem in Nette Framework",
  "keywords": [
    "nette",
    "doctrine",
    "dbal",
    "database",
    "extra"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-extra",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.1",
    "doctrine/dbal": "^3.6.5",
    "doctrine/orm": "^2.16.0"
  },
  "require-dev": {
    "ext-pdo": "*",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1",
    "mockery/mockery": "^1.6.4"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Extra\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
